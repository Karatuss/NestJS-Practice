{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "assets": [
      { "include": "src/config/*.yaml", "outDir": "dist/src/config/" },
      { "include": "src/db/schema/*.ts", "outDir": "dist/src/db/schema/" }
    ],
    "deleteOutDir": false
  }
}
